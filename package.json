{
  "name": "@u-iris/iris-front-ui",
  "version": "0.2.11-0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint --no-fix",
    "build": "vue-cli-service build --target lib --name iris-front-ui ./src/main.js --dest dist --mode production",
    "build:wc": "vue-cli-service build --target wc --name iris-front-ui-wc ./src/main-wc.js --dest dist-wc --mode production",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@vue/web-component-wrapper": "^1.2.0",
    "core-js": "^2.5.7",
    "material-design-icons-iconfont": "^4.0.4",
    "moment": "^2.24.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.6",
    "vue-avatar": "^2.1.7",
    "vue-cli-plugin-styleguidist": "^0.1.9",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^2.1.2",
    "vuetify": "^1.5.0"
  },
  "peerDependencies": {
    "element-ui": "^2.4.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/test-utils": "^1.0.0-beta.26",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-component": "^1.1.1",
    "element-ui": "^2.4.11",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "file-loader": "^3.0.1",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.0.5",
    "buble": "0.19.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/iris-front-ui.common.js"
}
